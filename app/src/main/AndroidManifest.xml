    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GoogleSuggest.Suggests"
            android:label="@string/title_activity_suggests"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DragAndDrop.DragAndDropMain" />
        <activity
            android:name=".tabDemo1.ShinTabMode"
            android:label="@string/title_activity_shin_tab_mode"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".musicMode.MusicMain">
            /></activity>

        <service android:name=".musicMode.MusicService" />

        <activity
            android:name=".musicMode.testactivity"
            android:label="@string/title_activity_testactivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>